generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
  output        = "./generated/mongo-client"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_DATABASE_URL")
}

model Supplier {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  schema String

  @@map("suppliers")
}
